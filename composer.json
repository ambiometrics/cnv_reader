{
    "name": "edwrodrig/cnv_parser",
    "description": "Library to parser CTD converted data files",
    "authors": [
        {
            "name": "Edwin Rodríguez León",
            "email": "edwrodrig@gmail.com",
            "homepage": "http://www.edwin.cl",
            "role": "Developer"
        }
    ],
    "keywords" : [
        "static-site-generator"
    ],
    "license" : "MIT",
    "minimum-stability" : "stable",
    "require": {
        "php" : "^7.2",
        "matthiasmullie/minify" : "^1.3",
        "leafo/scssphp": "^0.7.5",
        "phpdocumentor/reflection-docblock": "^4.3.0"
    },
    "require-dev" : {
        "phpunit/phpunit" : "^7.0",
        "edwrodrig/image" : "^2.0",
        "mikey179/vfsStream": "^1.6",
        "edwrodrig/contento" : "dev-master",
        "edwrodrig/js" : "^3.0"
    },
    "autoload" : {
        "psr-4" : {
            "edwrodrig\\cnv_parser\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "test\\edwrodrig\\cnv_parser\\" : "tests/"
        }
    },
    "archive": {
        "exclude": [
            "/tests",
            ".git",
            ".gitignore",
            "/.travis.yml",
            "/phpunit.xml.dist",
            "/examples",
            "/.idea",
            "README.md"
        ]
    }
}
